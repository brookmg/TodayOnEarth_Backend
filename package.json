{
  "name": "ollaapi",
  "version": "0.1.0",
  "description": "API to serve the universe",
  "main": "index.js",
  "scripts": {
    "queue": "rimraf dist && npm run build && node dist/queue/queue_sample.js",
    "build_c": "node-gyp rebuild && tsc && babel build --out-dir dist",
    "build": "tsc && babel build --out-dir dist",
    "test": "jest --coverage --silent",
    "dev": "nodemon --exec npm run restart",
    "dev_c": "nodemon --exec npm run restart_c",
    "restart": "rimraf dist && npm run build && npm run start_dev",
    "restart_c": "rimraf dist && npm run build_c && npm run start_dev",
    "knex:migrate": "npx knex migrate:latest --env development && npx knex seed:run --env development",
    "start": "npx knex migrate:latest --env production && node dist/index.js",
    "start_dev": "npx knex migrate:latest --env development && node dist/index.js",
    "doc:dev": "vuepress dev docs",
    "doc:build": "vuepress build docs"
  },
  "keywords": [
    "API",
    "Universe"
  ],
  "author": "Brook Mezgebu",
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/types": "^7.8.3",
    "@types/jest": "^25.1.2",
    "apollo-server": "^2.10.0",
    "apollo-server-express": "^2.10.1",
    "axios": "^0.19.2",
    "bcrypt": "^3.0.8",
    "bindings": "^1.5.0",
    "body-parser": "^1.19.0",
    "bull": "^3.12.1",
    "bull-arena": "^2.7.1",
    "cheerio": "^1.0.0-rc.3",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "fb": "^2.0.0",
    "graphql": "^14.6.0",
    "graphql-redis-subscriptions": "^2.1.2",
    "graphql-subscriptions": "^1.1.0",
    "jest": "^25.1.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.8",
    "lodash": "^4.17.15",
    "morgan": "^1.9.1",
    "node-addon-api": "^2.0.0",
    "node-telegram-login": "^1.0.0",
    "nodemailer": "^6.4.5",
    "nodemon": "^2.0.2",
    "objection": "^2.1.2",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-github2": "^0.1.11",
    "passport-google-oauth20": "^2.0.0",
    "passport-linkedin-oauth2": "^2.0.0",
    "passport-twitter": "^1.0.4",
    "pg": "^7.18.1",
    "request": "^2.88.2",
    "rimraf": "^3.0.1",
    "telegraf": "^3.36.0",
    "telegraph-node": "^1.0.4",
    "twitter": "^1.7.1",
    "typescript": "^3.8.3",
    "validator": "^12.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@types/node": "^13.7.0",
    "babel-core": "7.0.0-bridge.0",
    "vuepress": "^1.3.1"
  }
}
